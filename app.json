{
  "expo": {
    "name": "ReactProject",
    "slug": "ReactProject",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/assets/icon.png",
    "splash": {
      "image": "./app/assets/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "app/assets/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": ".app/assets/assets/favicon.png"
    }
  }
}
